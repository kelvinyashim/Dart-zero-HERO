class Person {
  String name;
  int age;
  String nationality;

  // Constructor
  Person({required this.name, required this.age, required this.nationality});

  // Getter methods
  String getName() {
    return name;
  }

  int getAge() {
    return age;
  }

  String getNationality() {
    return nationality;
  }

  // Setter methods
  void setName(String name) {
    this.name = name;
  }

  void setAge(int age) {
    this.age = age;
  }

  void setNationality(String nationality) {
    this.nationality = nationality;
  }
}
